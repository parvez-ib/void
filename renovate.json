{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "repositories": [
        "parvez-ib/void"
    ],
    "platform": "github",
    "extends": [
        ":enableVulnerabilityAlerts"
    ],
    "vulnerabilityAlerts": {
        "enabled": "true"
    },
    "osvVulnerabilityAlerts": true,
    "dryRun": false,
    "packageRules": [
        {
            "matchPackageNames": ["*"],
            "packagePatterns": ["^osv:"],
            "vulnerabilityAlerts": true
        }
    ],
    "constraints": {
        "python": "3.9"
    },
    "gitAuthor": "Renovate Bot <renovate@ib.com>",
    "major": false,
    "minor": true,
    "prConcurrentLimit": 5,
    "branchConcurrentLimit": 5
}
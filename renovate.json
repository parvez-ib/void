{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "repositories": [
    "parvez-ib/void"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["*"],
      "packagePatterns": ["^osv:"],
      "vulnerabilityAlerts": true
    }
  ],
  "force": {
    "constraints": {
        "python": "3.9",
    }
  }
}
